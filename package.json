{
    "name": "wordle-helper",
    "version": "1.0.0",
    "source": "src/index.ts",
    "main": "dist/index.js",
    "scripts": {
        "prebuild": "rm -rf dist && rm -rf bin",
        "build-x64": "parcel build && pkg dist/index.js -t node12-macos-x64 -o bin/wordle-helper",
        "build-arm": "parcel build && pkg dist/index.js -t node16-macos-arm64 -o bin/wordle-helper",
        "prelink": "rm -rf /usr/local/bin/wordle-helper",
        "link": "ln -s $(pwd)/bin/wordle-helper /usr/local/bin/"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "cli-progress": "^3.10.0",
        "node-localstorage": "^2.2.1",
        "node-persist": "^3.1.0",
        "prompts": "^2.4.2"
    },
    "devDependencies": {
        "@types/cli-progress": "^3.9.2",
        "@types/node-localstorage": "^1.3.0",
        "@types/node-persist": "^3.1.2",
        "@types/prompts": "^2.0.14",
        "parcel": "^2.5.0",
        "pkg": "^5.6.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/treetrum/wordle-helper.git"
    },
    "bugs": {
        "url": "https://github.com/treetrum/wordle-helper/issues"
    },
    "homepage": "https://github.com/treetrum/wordle-helper#readme",
    "description": ""
}
